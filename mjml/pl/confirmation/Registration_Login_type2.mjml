<mjml>
  <mj-head>
    <mj-title>Dzinga: Twoje nowe, błyszczące konto Dzinga tęskni za Tobą</mj-title>
    <mj-include path='./components/_head.mjml'/>
  </mj-head>
  <mj-body>
    <mj-wrapper>
      <!-- Top padding -->
      <mj-section mj-class='main_top_padding'></mj-section>
      <!-- Header // mj-section -->
      <mj-include path='./components/_header_full_logo.mjml'/>
      <!-- Main  -->
      <mj-section padding-top='30px' padding-bottom='4px'  mj-class='content_bg__white paddingX_10'>
        <mj-column>
          <mj-text mj-class='h1 black' align='left'>
            <strong>Twoje nowe, błyszczące konto Dzinga tęskni za Tobą</strong>
          </mj-text>
        </mj-column>
      </mj-section>
      <mj-section mj-class='content_bg__white paddingX_10' padding-top='6px' padding-bottom='6px'>
        <mj-column>
          <mj-text mj-class='main__text' align='left'>Twoja rejestracja została zakończona, ale niektóre funkcje Dzinga są nadal wyłączone dla Twojego konta.</mj-text>
        </mj-column>
      </mj-section>
      <mj-section padding-top='6px' padding-bottom='4px'  mj-class='content_bg__white paddingX_10'>
        <mj-column>
          <mj-text mj-class='h2' align='left'>… Nie musi tak być.</mj-text>
        </mj-column>
      </mj-section>
      <!-- Timeline // mj-section -->
      <mj-include path='./components/_timeline.mjml'/>
      <!-- Benefits // mj-section -->
      <mj-include path='./components/_separator.mjml'/>
      <mj-include path='./components/_features.mjml'/>
      <!-- Actions:Button // mj-section -->
      <mj-include path='./components/_separator.mjml'/>
      <mj-section mj-class='content_bg__white' padding-top='56px' padding-bottom='18px'>
        <mj-column>
          <mj-text mj-class='h2' align='center'>Gotowy, aby zwiększyć sprzedaż?</mj-text>
        </mj-column>
      </mj-section>
      <mj-include path='./components/_action_login.mjml'/>
      <mj-include path='./components/_action_inbrowser.mjml'/>
      <!-- Footer // mj-section -->
      <mj-include path='./components/_footer.mjml'/>
    </mj-wrapper>
  </mj-body>
</mjml>
